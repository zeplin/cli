{
  "name": "@zeplin/cli",
  "version": "0.3.1",
  "description": "Zeplin CLI",
  "main": "./dist/src/app",
  "types": "./dist/types/index",
  "bin": {
    "zeplin": "dist/src/app.js"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc && npm run prepare-api",
    "build:clean": "rm -rf dist/ && npm run build",
    "lint": "eslint --ext .js,.ts .",
    "prepare": "npm run build:clean",
    "prepare-api": "api-extractor run",
    "generate-docs": "api-documenter markdown --input-folder temp/ --output-folder docs/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zeplin/cli.git"
  },
  "bugs": {
    "url": "https://github.com/zeplin/cli/issues"
  },
  "homepage": "https://github.com/zeplin/cli#readme",
  "devDependencies": {
    "@microsoft/api-documenter": "^7.7.2",
    "@microsoft/api-extractor": "^7.7.0",
    "@types/ci-info": "^2.0.0",
    "@types/express": "^4.17.2",
    "@types/fs-extra": "^8.0.0",
    "@types/hapi__joi": "^16.0.3",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^24.0.18",
    "@types/jsonwebtoken": "^8.3.4",
    "@types/node": "^12.7.5",
    "@types/url-join": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "@zeplin/eslint-config": "^2.2.0",
    "eslint": "^6.4.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "@hapi/joi": "^16.1.7",
    "axios": "^0.19.0",
    "chalk": "^3.0.0",
    "ci-info": "^2.0.0",
    "commander": "^3.0.1",
    "endent": "^1.3.0",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "http-status-codes": "^1.4.0",
    "inquirer": "^7.0.0",
    "jsonwebtoken": "^8.5.1",
    "ts-dedent": "^1.1.0",
    "url-join": "^4.0.1"
  }
}
